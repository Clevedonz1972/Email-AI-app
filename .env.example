# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_API_TIMEOUT=30000

# AI Configuration
REACT_APP_AI_MODEL=gpt-4
REACT_APP_MAX_TOKENS=200

# Monitoring

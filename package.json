{
  "name": "email-ai-app",
  "version": "0.1.0",
  "private": true,
  "description": "Email AI Application",
  "main": "src/index.js",
  "proxy": "http://backend:8000",
  "scripts": {
    "start": "craco start",
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test:file": "craco test --testMatch='**/__tests__/**/*.test.{ts,tsx}'",
    "test:watch": "craco test --watch",
    "test:coverage": "craco test --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:a11y": "craco test --testMatch='**/*.a11y.test.tsx'",
    "test:perf": "craco test 'src/test/performance/**/*.test.tsx'",
    "test:all": "npm run test && npm run test:e2e && npm run test:a11y && npm run test:perf",
    "eject": "react-scripts eject",
    "vercel-build": "npm run build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.11.0",
    "@fullcalendar/daygrid": "^6.1.15",
